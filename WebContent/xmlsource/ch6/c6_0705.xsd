<?xml version="1.0" encoding="euc-kr"?>
<xsd:schema	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	
	<!-- 쐍풫 찏쟕쟷퀉 쨤쮫 -->
	<xsd:element name="친" type="xsd:string"/>
	
	
	<!-- 茅 찏쟕쟷퀉 쨤쮫 -->  
	<xsd:element name="친점">
		<xsd:complexType>
			<xsd:sequence>
				
				<!-- 쐍풫 찏쟕쟷퀉 츂촜 --> 
				<xsd:element ref="친" maxOccurs="unbounded"/>
				
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
</xsd:schema>
